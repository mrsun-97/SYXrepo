   "source": [
    "N = 1000000\n",
    "s = 1\n",
    "c=0 \n",
    "for i = 1:N\n",
    "    b = s*rand()\n",
    "    a = abs( s/4*cos(rand()*2π) )\n",
    "    if b+a>=s || b-a<=0\n",
    "        c += 1\n",
    "    end\n",
    "end\n",
    "println(\"π = \",N/c)\n"
   ]
